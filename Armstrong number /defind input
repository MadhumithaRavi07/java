public class Main {
    public static void main(String args[]) {
        int a = 364;
        int sum = 0;
        int num;

        while (a > 0) {
            num = a % 10;
            sum = sum + num * num * num;
            a = a / 10;
        }

        if (a == sum) {
            System.out.println("The number is an Armstrong number.");
        } else {
            System.out.println("Not an Armstrong number.");
        }
    }
}
